{
  "kind": "collectionType",
  "collectionName": "inventarios",
  "info": {
    "singularName": "inventario",
    "pluralName": "inventarios",
    "displayName": "Inventario",
    "description": "Inventario por producto y color"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "stock_actual": {
      "type": "biginteger",
      "default": 0
    },
    "producto": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::producto.producto"
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "required": true
    },
    "ultimo_ingreso": {
      "type": "datetime"
    },
    "codigo": {
      "type": "uid"
    },
    "ultima_salida": {
      "type": "datetime"
    },
    "stock_minimo": {
      "type": "integer",
      "default": 0
    },
    "unidad_de_medida": {
      "type": "enumeration",
      "enum": [
        "docena",
        "paquete"
      ],
      "default": "docena"
    }
  }
}